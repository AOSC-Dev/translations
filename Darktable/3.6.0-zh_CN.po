#: ../src/iop/toneequal.c:3083 ../src/iop/toneequal.c:3087
#: ../src/iop/toneequal.c:3091 ../src/iop/toneequal.c:3095
#: ../src/iop/toneequal.c:3099 ../src/iop/toneequal.c:3103
